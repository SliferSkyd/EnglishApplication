<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="528.0" prefWidth="769.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.englishapplication.SearchController">
   <children>
      <AnchorPane layoutX="301.0" layoutY="15.0" prefHeight="498.0" prefWidth="417.0" styleClass="search-box" stylesheets="@style/container.css">
         <children>
            <WebView fx:id="definitionView" layoutX="8.0" layoutY="7.0" prefHeight="481.0" prefWidth="431.0" />
            <Button layoutX="384.0" layoutY="6.0" mnemonicParsing="false" onAction="#starAction" prefHeight="46.0" prefWidth="58.0" styleClass="button-navigator" stylesheets="@style/container.css">
               <graphic>
                  <ImageView fx:id="star" fitHeight="45.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true" />
               </graphic></Button>
         </children></AnchorPane>
      <TextField fx:id="searchField" layoutX="17.0" layoutY="18.0" onKeyPressed="#searchFieldAction" prefHeight="46.0" prefWidth="267.0" promptText="Type your word..." styleClass="search-field" stylesheets="@style/container.css" />
      <ListView fx:id="listView" layoutX="17.0" layoutY="76.0" onMouseClicked="#listViewAction" prefHeight="383.0" prefWidth="267.0" stylesheets="@style/container.css" />
      <Button layoutX="17.0" layoutY="468.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="78.0" styleClass="btn-success" stylesheets="@style/container.css" text="Add" />
      <Button layoutX="112.0" layoutY="468.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="78.0" styleClass="btn-warning" stylesheets="@style/container.css" text="Edit" />
      <Button layoutX="206.0" layoutY="468.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="78.0" styleClass="btn-danger" stylesheets="@style/container.css" text="Delete" />
   </children>
</AnchorPane>
